<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:bind="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data class="SinglePlayResultBinding">

        <variable
            name="SinglePlayResultVm"
            type="kwizzapp.com.kwizzapp.viewmodels.SinglePlayResultVm" />
    </data>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".singleplay.SinglePlayResultFragment">

        <include
            layout="@layout/single_player_result_layout"
            bind:SinglePlayResultVm="@{SinglePlayResultVm}" />

    </FrameLayout>
</layout>
